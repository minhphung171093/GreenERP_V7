<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
		<record id="co_cau_rule" model="ir.rule">
            <field name="name">multi_company_co_cau_rule</field>
            <field model="ir.model" name="model_id" ref="model_co_cau"/>
            <field name="domain_force">['|',('company_id','=',False),('company_id','child_of',[user.company_id.id])]</field>
        </record>
        
        <record model="ir.module.category" id="module_category_cocaudan">
	        <field name="name">Cơ Cấu Đàn</field>
	        <field name="sequence">0</field>
	    </record>
		
		<record id="group_cocaudan_user" model="res.groups">
	        <field name="name">Người Dùng</field>
	        <field name="category_id" ref="module_category_cocaudan"/>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
	    </record>
        
        <record id="group_cocaudan_manager" model="res.groups">
	        <field name="name">Quản Lý</field>
	        <field name="category_id" ref="module_category_cocaudan"/>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="implied_ids" eval="[(4, ref('green_erp_ccty_cocaudan.group_cocaudan_user'))]"/>
	    </record>
	    
        <record id="green_erp_ccty_base.group_base_admin_ccty" model="res.groups">
	        <field name="implied_ids" eval="[(4, ref('green_erp_ccty_cocaudan.group_cocaudan_manager'))]"/>
	    </record>
	</data>
</openerp>

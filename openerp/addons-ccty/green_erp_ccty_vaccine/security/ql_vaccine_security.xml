<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
        
        <record model="ir.module.category" id="module_category_vaccine">
	        <field name="name">Vaccine</field>
	        <field name="sequence">0</field>
	    </record>
		
		<record id="group_vaccine_user" model="res.groups">
	        <field name="name">Người Dùng</field>
	        <field name="category_id" ref="module_category_vaccine"/>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
	    </record>
        
        <record id="group_vaccine_manager" model="res.groups">
	        <field name="name">Quản Lý</field>
	        <field name="category_id" ref="module_category_vaccine"/>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="implied_ids" eval="[(4, ref('green_erp_ccty_vaccine.group_vaccine_user'))]"/>
	    </record>
	    
        <record id="green_erp_ccty_base.group_base_admin_ccty" model="res.groups">
	        <field name="implied_ids" eval="[(4, ref('green_erp_ccty_vaccine.group_vaccine_manager'))]"/>
	    </record>
	    
	</data>
</openerp>

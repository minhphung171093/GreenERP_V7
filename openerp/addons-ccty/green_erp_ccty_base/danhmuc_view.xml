        </record>

        					
        					 
        					<!--Loài vật-->   



    <record id="loai_vat_search" model="ir.ui.view">
	        <field name="name">loai.vat.search</field>
	        <field name="model">loai.vat</field>
	        <field name="type">search</field>
	        <field name="arch" type="xml">
	            <search string="Loài vật">
	                <group>
	                    <field name="ma_loai" />
	                    <field name="name" />
	                    <field name="thuoc_loai" />                    
	                    <field name="thoi_gian" />
	                </group>
	            </search>	
	        </field>
	    </record>
    
        <record model="ir.ui.view" id="loai_vat_tree">
            <field name="name">loai.vat.tree</field>
            <field name="model">loai.vat</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Tree Loài vật">
                    <field name="ma_loai" invisible = '1'/>
                    <field name="name" />
                    <field name="thuoc_loai" />                    
                    <field name="thoi_gian" />
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="loai_vat_form">
            <field name="name">loai.vat.form</field>
            <field name="model">loai.vat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="form Loài vật" version="7.0">
	                <sheet>
                		<group>
			            	<field name="ma_loai" invisible = '1'/>
		                    <field name="name"/>
		                    <field name="thuoc_loai" />                    
		                    <field name="thoi_gian" />
                		</group>
                		<notebook>
							<page string = "Thông tin chi tiết">
								<field name="chitiet_loaivat" nolabel='1' >
									<tree editable='bottom'>
										<field name="name" />
										<field name="tiem_phong" />
									</tree>
								</field>
							</page>
							<page string = "Thông tin chi tiết các vaccine">
								<field name="chitiet_loai_vaccine" nolabel='1' >
									<tree editable='bottom'>
										<field name="vacxin_id" options="{'create': false, 'create_edit': false}"/>
										<field name="yes_no"/>
									</tree>
								</field>
							</page>
						</notebook>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_loai_vat">
            <field name="name">Danh Mục Loài vật</field>
            <field name="res_model">loai.vat</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        					
        					<!--hộ chăn nuôi-->   

	    <record id="chan_nuoi_search" model="ir.ui.view">
            <field name="name">chan.nuoi.search</field>
            <field name="model">chan.nuoi</field>
            <field name="arch" type="xml">
                <search string="Hộ chăn nuôi">
                	<field name="name" />
                    <field name="ma_ho" />
                    <field name="so_nha" />                    
                    <field name="ngay_cap" />
                    <field name="quan_huyen_id" />
                    <field name="phuong_xa_id" />
                    <field name="khu_pho_id" />
                    <field name="dien_tich" />
                    <field name="dien_tich_chuong" />
                    <field name="an_toan_dich" />
                    <field name="loai_ho_id" />
                    <field name="company_id" />
                    <separator/>
                    <group expand="0" string="Group By...">
                        <filter string="Quận (huyện)" domain="[]" context="{'group_by':'quan_huyen_id'}"/>
                        <filter string="Phường (xã)" domain="[]"  context="{'group_by':'phuong_xa_id'}"/>
                        <filter string="Khu phố (ấp)" domain="[]" context="{'group_by':'khu_pho_id'}"/>
                        <filter string="Thời gian cấp" domain="[]"  context="{'group_by':'ngay_cap'}"/>
                        <filter string="An toàn dịch" domain="[]"  context="{'group_by':'an_toan_dich'}"/>
                        <filter string="Loại hộ" domain="[]"  context="{'group_by':'loai_ho_id'}"/>
                        <filter string="Trạm" domain="[]"  context="{'group_by':'company_id'}"/>
                    </group>
               </search>
            </field>
        </record>
    
        <record model="ir.ui.view" id="chan_nuoi_tree">
            <field name="name">chan.nuoi.tree</field>
            <field name="model">chan.nuoi</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Hộ chăn nuôi">
                    <field name="name" />
                    <field name="ma_ho" />
                    <field name="ngay_cap" />                 
                    <field name="company_id" />
                    <field name="loai_ho_id" />
                    <field name="dien_thoai" />
                    <field name="so_nha" />  
                    <field name="quan_huyen_id" options="{'create': false, 'create_edit': false}"/>
                    <field name="phuong_xa_id" domain="[('quan_huyen_id','=',quan_huyen_id)]" options="{'create': false, 'create_edit': false}"/>
                    <field name="khu_pho_id" domain="[('phuong_xa_id','=',phuong_xa_id)]" options="{'create': false, 'create_edit': false}"/>
                    <field name="dien_tich" />
                    <field name="dien_tich_chuong" />
                    <field name="an_toan_dich" />
                    <field name="lat" />
                    <field name="lng" />
                    <field name="trang_thai_id" options="{'create': false, 'create_edit': false}"/>
                    <field name="hien_an" invisible = '1'/>
                    <field name="chinh_sua_rel" invisible = '1'/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="chan_nuoi_form">
            <field name="name">chan.nuoi.form</field>
            <field name="model">chan.nuoi</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Hộ chăn nuôi" version="7.0">
                	<header>
	            		<button name="bt_duyet" attrs="{'invisible': [('hien_an', '!=', True)]}" string="Duyệt" type="object" class="oe_highlight" />
	            		
	                    <field name="trang_thai_id" widget="statusbar"/>
	            	</header> 
	                <sheet>
                		<group col="4"  string="Thông tin chung">
                			<field name="loai_ho_id" options="{'create': false, 'create_edit': false}" string="Loại cơ sở" />
		            		<field name="name" string="Tên trại, cơ sở" />
		                    <field name="ma_ho" string="Mã trại, cơ sở" />
		                    <field name="dien_thoai"/>
		                    <field name="ngay_cap" />
		                    <field name="company_id" options="{'create': false, 'create_edit': false}" />
		                    <field name="hien_an" invisible = '1'/>
		                    <field name="chinh_sua_rel" invisible = '1' />
                		</group>
                		<group col="4"  string="Địa chỉ">
							<field name="so_nha" />                    
							<field name="quan_huyen_id" options="{'create': false, 'create_edit': false}" />
							<field name="phuong_xa_id" options="{'create': false, 'create_edit': false}" domain="[('quan_huyen_id','=',quan_huyen_id)]" />
							<field name="khu_pho_id" options="{'create': false, 'create_edit': false}" domain="[('phuong_xa_id','=',phuong_xa_id)]" />
							<field name="dien_tich" />
							<field name="dien_tich_chuong" />
							<field name="lat" />
							<field name="lng" />
                		</group>
                		<group col="4"  string="Mô tả">
							<field name="mo_ta" widget="ckeditor" nolabel='1'/>                    
                		</group>
                		<notebook>
							<page string = "Thông tin khác">
								<group col="4" string="Thông tin khác">
				                    <field name="loai_hinh_so_huu_id" options="{'create': false, 'create_edit': false}"  />       
				                    <field name="cty_gia_cong_ids" widget="many2many_tags"  />             
				                    <field name="quy_cach"  />
				                    <field name="xu_ly_moi_truong_ids" widget="many2many_tags" />
				                    <field name="bao_ve_moi_truong" />
				                    <field name="danh_gia_moi_truong"  />
				                    <field name="san_xuat_giong_id" string='Hướng sản xuất' />
                				</group>
                				<group string="Các tiêu chuẩn khác">
				                    <field name="tieu_chuan_viet" />
				                    <field name="tieu_chuan_global"  />
				                    <field name="an_toan_dich" />
				                    <field name="du_dk_thu_y" />
				                    <field name="tieu_chuan_khac"  />
                				</group>
							</page>
						</notebook>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_chan_nuoi">
            <field name="name">Danh mục cơ sở chăn nuôi</field>
            <field name="res_model">chan.nuoi</field>

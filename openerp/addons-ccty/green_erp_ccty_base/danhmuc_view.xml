<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    						 <!--cán bộ-->        
    
    <record id="can_bo_search" model="ir.ui.view">
	        <field name="name">can.bo.search</field>
	        <field name="model">can.bo</field>
	        <field name="type">search</field>
	        <field name="arch" type="xml">
	            <search string="Cán bộ">
	                <group>
	                    <field name="name" />
	                    <field name="tram_id" />                    
	                    <field name="email" />
	                    <field name="dien_thoai" />
	                </group>
	            </search>	
	        </field>
	    </record>
    
        <record model="ir.ui.view" id="can_bo_tree">
            <field name="name">can.bo.tree</field>
            <field name="model">can.bo</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Tree cán bộ">
                    <field name="name" />
                    <field name="tram_id" />                    
                    <field name="email" />
                    <field name="dien_thoai" />
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="can_bo_form">
            <field name="name">can.bo.form</field>
            <field name="model">can.bo</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="form cán bộ" version="7.0">
	                <sheet>
                		<group>
		            		<field name="name" />
		                    <field name="tram_id" />                    
		                    <field name="email" />
		                    <field name="dien_thoai" />
                		</group>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_can_bo">
            <field name="name">Danh Mục Cán Bộ</field>
            <field name="res_model">can.bo</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        
        					<!--Trạm thú y-->  
        					
   
    <record id="tram_thu_y_search" model="ir.ui.view">
	        <field name="name">tram.thu.y.search</field>
	        <field name="model">tram.thu.y</field>
	        <field name="type">search</field>
	        <field name="arch" type="xml">
	            <search string="Trạm thú y">
	                <group>
	                    <field name="name" />
	                    <field name="thanh_pho_id" />
	                    <field name="dia_chi" />                    
	                    <field name="email" />
	                    <field name="dien_thoai" />
	                    <field name="tram_truong_id" />
	                    <field name="toa_do_x" />
	                    <field name="toa_do_y" />
	                </group>
	            </search>	
	        </field>
	    </record>
    
        <record model="ir.ui.view" id="tram_thu_y_tree">
            <field name="name">tram.thu.y.tree</field>
            <field name="model">tram.thu.y</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Tree trạm thú y">
                	<field name="name" />
                    <field name="thanh_pho_id" />
                    <field name="dia_chi" />                    
                    <field name="email" />
                    <field name="dien_thoai" />
                    <field name="tram_truong_id" />
                    <field name="toa_do_x" />
                    <field name="toa_do_y" />
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="tram_thu_y_form">
            <field name="name">tram.thu.y.form</field>
            <field name="model">tram.thu.y</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="form trạm thú y" version="7.0">
	                <sheet>
                		<group col="4"  string="Thông tin trạm thú y">
			            		<field name="name" />
			            		<field name="email" />
			                    <field name="thanh_pho_id" />
			                    <field name="dien_thoai" />
			                    <field name="dia_chi" />                    
			                    <field name="tram_truong_id" />
			                    <field name="toa_do_x" />
			                    <field name="toa_do_y" />
                		</group>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_tram_thu_y">
            <field name="name">Danh Mục Trạm Thú Y</field>
            <field name="res_model">tram.thu.y</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        					
        					 
        					<!--Loài vật-->   



    <record id="loai_vat_search" model="ir.ui.view">
	        <field name="name">loai.vat.search</field>
	        <field name="model">loai.vat</field>
	        <field name="type">search</field>
	        <field name="arch" type="xml">
	            <search string="Loài vật">
	                <group>
	                    <field name="ma_loai" />
	                    <field name="name" />
	                    <field name="thuoc_loai" />                    
	                    <field name="thoi_gian" />
	                </group>
	            </search>	
	        </field>
	    </record>
    
        <record model="ir.ui.view" id="loai_vat_tree">
            <field name="name">loai.vat.tree</field>
            <field name="model">loai.vat</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Tree Loài vật">
                    <field name="ma_loai" />
                    <field name="name" />
                    <field name="thuoc_loai" />                    
                    <field name="thoi_gian" />
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="loai_vat_form">
            <field name="name">loai.vat.form</field>
            <field name="model">loai.vat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="form Loài vật" version="7.0">
	                <sheet>
                		<group>
			            	<field name="ma_loai" />
		                    <field name="name" />
		                    <field name="thuoc_loai" />                    
		                    <field name="thoi_gian" />
                		</group>
                		<notebook>
							<page string = "Thông tin chi tiết">
								<field name="chitiet_loaivat" nolabel='1'>
									<tree editable='bottom'>
										<field name="name" />
									</tree>
								</field>
							</page>
							<page string = "Thông tin chi tiết loại bệnh">
								<field name="chitiet_loaibenh" nolabel='1'>
									<tree editable='bottom'>
										<field name="name" />
									</tree>
								</field>
							</page>
						</notebook>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_loai_vat">
            <field name="name">Danh Mục Loài vật</field>
            <field name="res_model">loai.vat</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        					
        					<!--hộ chăn nuôi-->   

	    <record id="chan_nuoi_search" model="ir.ui.view">
            <field name="name">chan.nuoi.search</field>
            <field name="model">chan.nuoi</field>
            <field name="arch" type="xml">
                <search string="Hộ chăn nuôi">
                	<field name="name" />
                    <field name="ma_ho" />
                    <field name="so_nha" />                    
                    <field name="ngay_cap" />
                    <field name="quan_huyen_id" />
                    <field name="phuong_xa_id" />
                    <field name="khu_pho_id" />
                    <field name="dien_tich" />
                    <separator/>
                    <group expand="0" string="Group By...">
                        <filter string="Quận (huyện)" domain="[]" context="{'group_by':'quan_huyen_id'}"/>
                        <filter string="Phường (xã)" domain="[]"  context="{'group_by':'phuong_xa_id'}"/>
                        <filter string="Khu phố (ấp)" domain="[]" context="{'group_by':'khu_pho_id'}"/>
                        <filter string="Thời gian cấp" domain="[]"  context="{'group_by':'ngay_cap'}"/>
                    </group>
               </search>
            </field>
        </record>
    
        <record model="ir.ui.view" id="chan_nuoi_tree">
            <field name="name">chan.nuoi.tree</field>
            <field name="model">chan.nuoi</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Hộ chăn nuôi">
                    <field name="name" />
                    <field name="ma_ho" />
                    <field name="so_nha" />                    
                    <field name="ngay_cap" />
                    <field name="quan_huyen_id" />
                    <field name="phuong_xa_id" domain="[('quan_huyen_id','=',quan_huyen_id)]"/>
                    <field name="khu_pho_id" domain="[('phuong_xa_id','=',phuong_xa_id)]"/>
                    <field name="dien_tich" />
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="chan_nuoi_form">
            <field name="name">chan.nuoi.form</field>
            <field name="model">chan.nuoi</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Hộ chăn nuôi" version="7.0">
	                <sheet>
                		<group col="4"  string="Thông tin hộ chăn nuôi">
		            		<field name="name" />
		                    <field name="ma_ho" />
		                    <field name="so_nha" />                    
		                    <field name="ngay_cap" />
		                    <field name="quan_huyen_id" />
		                    <field name="phuong_xa_id" />
		                    <field name="khu_pho_id" />
		                    <field name="dien_tich" />
                		</group>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_chan_nuoi">
            <field name="name">Danh Mục Hộ Chăn Nuôi</field>
            <field name="res_model">chan.nuoi</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field eval="False" name="view_id" ref="chan_nuoi_tree"/>
            <field name="search_view_id" ref="chan_nuoi_search"/>
        </record>

        					
        					
        					

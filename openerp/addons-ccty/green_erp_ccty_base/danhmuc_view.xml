<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    						 <!--cán bộ-->        
    
        <record id="res_users_canbo_form_inherit" model="ir.ui.view">
            <field name="name">res.users.canbo.form.inherit</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='company_id']" position="after">
					<field name="dien_thoai"/>
				</xpath>
				<xpath expr="//field[@name='company_id']" position="attributes">
					<attribute name='string'>Trạm</attribute>
				</xpath>
				<xpath expr="//label[@for='name']" position="attributes">
					<attribute name='string'>Tên cán bộ</attribute>
				</xpath>
				<xpath expr="//label[@for='login']" position="attributes">
					<attribute name='string'>Tên đăng nhập</attribute>
				</xpath>
				
			</field>
		</record>
		
		<record id="action_res_users_canbo" model="ir.actions.act_window">
            <field name="name">Cán Bộ</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.users</field>
            <field name="view_type">form</field>
            <field name="view_id" ref="base.view_users_tree"/>
            <field name="domain">[('id','!=',1)]</field>
            <field name="context">{}</field>
            <field name="search_view_id" ref="base.view_users_search"/>
            <field name="help">Create and manage users that will connect to the system. Users can be deactivated should there be a period of time during which they will/should not connect to the system. You can assign them groups in order to give them specific access to the applications they need to use in the system.</field>
        </record>
        <record id="action_res_users_canbo1" model="ir.actions.act_window.view">
            <field eval="10" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="base.view_users_tree"/>
            <field name="act_window_id" ref="action_res_users_canbo"/>
        </record>
        <record id="action_res_users_canbo2" model="ir.actions.act_window.view">
            <field eval="20" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="res_users_canbo_form_inherit"/>
            <field name="act_window_id" ref="action_res_users_canbo"/>
        </record>
       
        					<!--Trạm thú y-->  
        					
   
    <record id="res_company_form_inherit" model="ir.ui.view">
            <field name="name">res.company.form.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
				<xpath expr="//label[@for='name']" position="attributes">
					<attribute name='string'>Tên trạm</attribute>
				</xpath>
				<xpath expr="//field[@name='parent_id']" position="attributes">
					<attribute name='string'>Trạm cha</attribute>
				</xpath>
				
				<xpath expr="//field[@name='parent_id']" position="after">
					<field name="cap" required = '1'/>
				</xpath>
				
				<xpath expr="//group[@string='Bank Accounts']" position="attributes">
					<attribute name='invisible'>1</attribute>
				</xpath>
				
				<xpath expr="//group[@string='Report Footer Configuration']" position="attributes">
					<attribute name='invisible'>1</attribute>
				</xpath>
				
				<xpath expr="//page[@string='Header/Footer']" position="attributes">
					<attribute name='invisible'>1</attribute>
				</xpath>
				
				<xpath expr="//page[@string='Configuration']" position="attributes">
					<attribute name='invisible'>1</attribute>
				</xpath>
				
				<xpath expr="//page[@string='General Information']/group/group/field[@name='vat']" position="attributes">
					<attribute name='invisible'>1</attribute>
				</xpath>
				
				<xpath expr="//page[@string='General Information']/group/group/field[@name='company_registry']" position="attributes">
					<attribute name='invisible'>1</attribute>
				</xpath>
				
			</field>
		</record>
       
        <record model="ir.actions.act_window" id="action_tram_thu_y">
            <field name="name">Danh Mục Trạm Thú Y</field>
            <field name="res_model">tram.thu.y</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        					
        					 
        					<!--Loài vật-->   



    <record id="loai_vat_search" model="ir.ui.view">
	        <field name="name">loai.vat.search</field>
	        <field name="model">loai.vat</field>
	        <field name="type">search</field>
	        <field name="arch" type="xml">
	            <search string="Loài vật">
	                <group>
	                    <field name="ma_loai" />
	                    <field name="name" />
	                    <field name="thuoc_loai" />                    
	                    <field name="thoi_gian" />
	                </group>
	            </search>	
	        </field>
	    </record>
    
        <record model="ir.ui.view" id="loai_vat_tree">
            <field name="name">loai.vat.tree</field>
            <field name="model">loai.vat</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Tree Loài vật">
                    <field name="ma_loai" />
                    <field name="name" />
                    <field name="thuoc_loai" />                    
                    <field name="thoi_gian" />
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="loai_vat_form">
            <field name="name">loai.vat.form</field>
            <field name="model">loai.vat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="form Loài vật" version="7.0">
	                <sheet>
                		<group>
			            	<field name="ma_loai" />
		                    <field name="name" />
		                    <field name="thuoc_loai" />                    
		                    <field name="thoi_gian" />
                		</group>
                		<notebook>
							<page string = "Thông tin chi tiết">
								<field name="chitiet_loaivat" nolabel='1'>
									<tree editable='bottom'>
										<field name="name" />
									</tree>
								</field>
							</page>
							<page string = "Thông tin chi tiết loại bệnh">
								<field name="chitiet_loaibenh" nolabel='1'>
									<tree editable='bottom'>
										<field name="name" />
									</tree>
								</field>
							</page>
						</notebook>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_loai_vat">
            <field name="name">Danh Mục Loài vật</field>
            <field name="res_model">loai.vat</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        					
        					<!--hộ chăn nuôi-->   

	    <record id="chan_nuoi_search" model="ir.ui.view">
            <field name="name">chan.nuoi.search</field>
            <field name="model">chan.nuoi</field>
            <field name="arch" type="xml">
                <search string="Hộ chăn nuôi">
                	<field name="name" />
                    <field name="ma_ho" />
                    <field name="so_nha" />                    
                    <field name="ngay_cap" />
                    <field name="quan_huyen_id" />
                    <field name="phuong_xa_id" />
                    <field name="khu_pho_id" />
                    <field name="dien_tich" />
                    <separator/>
                    <group expand="0" string="Group By...">
                        <filter string="Quận (huyện)" domain="[]" context="{'group_by':'quan_huyen_id'}"/>
                        <filter string="Phường (xã)" domain="[]"  context="{'group_by':'phuong_xa_id'}"/>
                        <filter string="Khu phố (ấp)" domain="[]" context="{'group_by':'khu_pho_id'}"/>
                        <filter string="Thời gian cấp" domain="[]"  context="{'group_by':'ngay_cap'}"/>
                    </group>
               </search>
            </field>
        </record>
    
        <record model="ir.ui.view" id="chan_nuoi_tree">
            <field name="name">chan.nuoi.tree</field>
            <field name="model">chan.nuoi</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Hộ chăn nuôi">
                    <field name="name" />
                    <field name="ma_ho" />
                    <field name="so_nha" />                    
                    <field name="ngay_cap" />
                    <field name="quan_huyen_id" options="{'create': false, 'create_edit': false}"/>
                    <field name="phuong_xa_id" domain="[('quan_huyen_id','=',quan_huyen_id)]" options="{'create': false, 'create_edit': false}"/>
                    <field name="khu_pho_id" domain="[('phuong_xa_id','=',phuong_xa_id)]" options="{'create': false, 'create_edit': false}"/>
                    <field name="dien_tich" />
                    <field name="trang_thai_id" options="{'create': false, 'create_edit': false}"/>
                    <field name="hien_an" invisible = '1'/>
                    <field name="chinh_sua_rel" invisible = '1'/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="chan_nuoi_form">
            <field name="name">chan.nuoi.form</field>
            <field name="model">chan.nuoi</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Hộ chăn nuôi" version="7.0">
	                <sheet>
	                <header>
	            		<button name="bt_duyet" groups = "green_erp_ccty_base.group_base_admin_ccty" attrs="{'invisible': [('hien_an', '!=', True)]}" string="Duyệt" type="object" class="oe_highlight" />
	                    <field name="trang_thai_id" widget="statusbar"/>
	            	</header> 
                		<group col="4"  string="Thông tin hộ chăn nuôi">
		            		<field name="name" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="ma_ho" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="so_nha" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>                    
		                    <field name="ngay_cap" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="quan_huyen_id" options="{'create': false, 'create_edit': false}" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="phuong_xa_id" options="{'create': false, 'create_edit': false}" domain="[('quan_huyen_id','=',quan_huyen_id)]" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="khu_pho_id" options="{'create': false, 'create_edit': false}" domain="[('phuong_xa_id','=',phuong_xa_id)]" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="dien_tich" attrs="{'readonly': [('chinh_sua_rel', 'not in', ['nhap', False])]}"/>
		                    <field name="hien_an" invisible = '1'/>
		                    <field name="chinh_sua_rel" invisible = '1' />
                		</group>
	                </sheet>
                </form>
            </field>
        </record>
       
        <record model="ir.actions.act_window" id="action_chan_nuoi">
            <field name="name">Danh Mục Hộ Chăn Nuôi</field>
            <field name="res_model">chan.nuoi</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field eval="False" name="view_id" ref="chan_nuoi_tree"/>
            <field name="search_view_id" ref="chan_nuoi_search"/>
        </record>

        					
        					
        					

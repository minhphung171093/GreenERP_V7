            <field name="model">trangthai.nhom.congviec</field>
            <field name="arch" type="xml">
                <search string="Trạng thái của nhóm công việc">
                   <field name="name" string="Project Stages"/>
                   <filter icon="terp-check" string="Common" name="common" domain="[('case_default', '=', 1)]" help="Stages common to all projects"/>
                </search>
            </field>
        </record>

        <record id="trangthai_nhom_congviec_edit" model="ir.ui.view">

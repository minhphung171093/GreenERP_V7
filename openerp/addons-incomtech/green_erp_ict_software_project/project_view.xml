            <field name="arch" type="xml">
                <tree string="Nhóm công việc">
                    <field name="message_unread" invisible="1"/>
                    <field name="name"/>
                    <field name="duan_id"/>
                    <field name="user_id"/>
                    <field name="ngay_bd"/>

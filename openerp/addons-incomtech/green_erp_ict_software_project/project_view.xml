		<!-- Dự án -->
		<record id="duan_form_view" model="ir.ui.view">
            <field name="name">duan.phanmem.form</field>
            <field name="model">duan.phanmem</field>
            <field name="arch" type="xml">
                <form string="Dự án" version="7.0">
                <header>
                    <field name="stage_id" widget="statusbar" clickable="True"/>
                </header>
                <sheet string="Dự án">
                    <div class="oe_title">
                        <label for="name" class="oe_edit_only"/>
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <div class="oe_right oe_button_box" name="buttons" groups="base.group_user">
                        <button name="%(action_project_nhom_congviec)d" string="Nhóm công việc"
                            type="action"/>
                        <button name="%(action_project_congviec)d" string="Công việc"
                            type="action"/>
                    </div>
                    <group>
                        <group>
                            <field name="user_id" context="{'search_user_for_project': 1}"/>
                            <field name="ngay_bd"/>
                        	<field name="ngay_kt"/>
                        	<field name="ngay_ht"/>
                        </group>
                        <group>
                        	<field name="han_chot"/>
                        	<field name="muc_thuong"/>
                        	<field name="sotien_giam"/>
                        </group>
                    </group>
                    <notebook>
                    	<page string="Nhóm">
                            <field name="team_line" nolabel='1'>
                            	<tree editable='1'>
                            		<field name="user_id"/>
                        			<field name="he_so"/>
                        			<field name="thanhtien" sum="Tổng tiền"/>
                            	</tree>
                            </field>
                            <field name="members" invisible='1'/>
                        </page>
                        <page string="Mô tả">
                            <field name="description" nolabel='1'/>
                        </page>
                        <page string="Trạng thái của dự án">
                            <field name="trangthai_duan_ids" nolabel='1'/>
                        </page>
                        <page string="Trạng thái của công việc">
                            <field name="trangthai_congviec_ids" nolabel='1'/>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
                </form>
            </field>
        </record>


                            <field name="fold"/>
                        </group>
                    </group>
                    <field name="description" placeholder="Add a description..."/>
                </form>
            </field>
        </record>

        <record id="trangthai_nhom_congviec_tree" model="ir.ui.view">
            <field name="name">trangthai.nhom.congviec.tree</field>
            <field name="model">trangthai.nhom.congviec</field>
            <field name="arch" type="xml">
                <tree string="Trạng thái của nhóm công việc">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="action_trangthai_nhom_congviec" model="ir.actions.act_window">
            <field name="name">Trạng thái của nhóm công việc</field>
            <field name="res_model">trangthai.nhom.congviec</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="trangthai_nhom_congviec_tree"/>
        </record>
        
        <!-- Trạng thái của công việc -->
        <record id="trangthai_congviec_search" model="ir.ui.view">
            <field name="name">trangthai.congviec.search</field>
            <field name="model">trangthai.congviec</field>
            <field name="arch" type="xml">
                <search string="Trạng thái của công việc">
                   <field name="name" string="Project Stages"/>
                   <filter icon="terp-check" string="Common" name="common" domain="[('case_default', '=', 1)]" help="Stages common to all projects"/>
                </search>
            </field>
        </record>

        <record id="trangthai_congviec_edit" model="ir.ui.view">
            <field name="name">trangthai.congviec.form</field>
            <field name="model">trangthai.congviec</field>
            <field name="arch" type="xml">
                <form string="Trạng thái của công việc" version="7.0">
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="case_default"/>
                        </group>
                        <group>
                            <field name="state"/>

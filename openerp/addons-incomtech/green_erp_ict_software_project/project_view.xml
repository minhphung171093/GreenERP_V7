                            <field name="fold"/>
                        </group>
                    </group>
                    <field name="description" placeholder="Add a description..."/>
                </form>
            </field>
        </record>

        <record id="trangthai_congviec_tree" model="ir.ui.view">
            <field name="name">trangthai.congviec.tree</field>
            <field name="model">trangthai.congviec</field>
            <field name="arch" type="xml">
                <tree string="Trạng thái của công việc">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="action_trangthai_congviec" model="ir.actions.act_window">
            <field name="name">Trạng thái của công việc</field>
            <field name="res_model">trangthai.congviec</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="trangthai_congviec_tree"/>
        </record>
    	
    	<!-- Công việc -->
		<record id="congviec_form_view" model="ir.ui.view">
            <field name="name">cong.viec.form</field>
            <field name="model">cong.viec</field>
            <field name="arch" type="xml">
                <form string="Công việc" version="7.0">
                <header>
                    <field name="stage_id" widget="statusbar" clickable="True"/>
                </header>
                <sheet string="Công việc">
                    <div class="oe_title">
                        <label for="name" class="oe_edit_only"/>
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <div class="oe_right oe_button_box" name="buttons" groups="base.group_user">
                        <!--<button name="attachment_tree_view" string="Documents" type="object"/>-->
                    </div>
                    <group>
                        <group>
                            <field name="duan_id"/>
                            <field name="nhom_congviec_id" domain="[('duan_id','=',duan_id)]"/>
                            <field name="user_id" context="{'search_user_with_project': duan_id}"/>
                            <field name="priority"/>
                   			<field name="sequence"/>
                        </group>
                        <group>
                        	<field name="ngay_bd"/>
                        	<field name="ngay_kt"/>
                        	<field name="ngay_ht"/>
                        	<field name="han_chot"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Mô tả">
                            <field name="description" nolabel='1'/>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
                </form>
            </field>
        </record>

        <record id="congviec_search_view" model="ir.ui.view">
            <field name="name">cong.viec.search</field>
            <field name="model">cong.viec</field>
            <field name="arch" type="xml">
                <search string="Công việc">
                    <field name="name"/>
                    <filter icon="terp-mail-message-new" string="Unread Messages" name="message_unread" domain="[('message_unread','=',True)]"/>
                    <separator/>
                    <filter string="Công việc của tôi" name="my_congviec" domain="[('user_id','=',uid)]"/>
                    <field name="duan_id"/>
                    <field name="nhom_congviec_id"/>
                    <field name="user_id"/>
                    <field name="ngay_bd"/>
                    <field name="ngay_kt"/>
                    <field name="han_chot"/>
                    <field name="priority"/>
                    <field name="sequence"/>
                    <field name="stage_id"/>
                    <group expand="0" string="Group By...">
                        <filter string="Dự án" name="duan" icon="terp-personal" domain="[]" context="{'group_by':'duan_id'}"/>
                        <filter string="Nhóm công việc" name="nhom_congviec" icon="terp-personal" domain="[]" context="{'group_by':'nhom_congviec_id'}"/>
                        <filter string="Người thực hiện" name="manager" icon="terp-personal" domain="[]" context="{'group_by':'user_id'}"/>
                        <filter string="Trạng thái" name="trangthai" icon="terp-personal" domain="[]" context="{'group_by':'stage_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="congviec_tree_view" model="ir.ui.view">
            <field name="name">cong.viec.tree</field>
            <field name="model">cong.viec</field>
            <field name="arch" type="xml">
                <tree string="Công việc">
                    <field name="message_unread" invisible="1"/>
                    <field name="name"/>
                    <field name="duan_id"/>
                    <field name="nhom_congviec_id"/>
                    <field name="user_id"/>
                    <field name="ngay_bd"/>
                    <field name="ngay_kt"/>
                    <field name="han_chot"/>
                    <field name="priority"/>
                    <field name="sequence"/>
                    <field name="stage_id"/>
                </tree>
            </field>
        </record>
        
    	<record id="congviec_calendar_view" model="ir.ui.view">
            <field name="name">cong.viec.calendar</field>
            <field name="model">cong.viec</field>
            <field name="arch" type="xml">
                <calendar color="user_id" date_start="ngay_bd" date_stop="ngay_kt" string="Công việc">
                    <field name="name"/>
                    <field name="duan_id"/>
                    <field name="nhom_congviec_id"/>
                </calendar>
            </field>
        </record>
        
        <record model="ir.ui.view" id="congviec_kanban_view">
            <field name="name">cong.viec.kanban</field>
            <field name="model">cong.viec</field>
            <field name="arch" type="xml">
                <kanban default_group_by="stage_id">
                    <field name="name"/>
                    <field name="duan_id"/>
                    <field name="nhom_congviec_id"/>
                    <field name="user_id"/>
                    <field name="ngay_bd"/>
                    <field name="ngay_kt"/>
                    <field name="han_chot"/>
                    <field name="color"/>
                    <field name="priority"/>
                    <field name="sequence"/>
                    <field name="stage_id"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_color_#{kanban_getcolor(record.color.raw_value)} oe_kanban_card oe_kanban_project oe_kanban_global_click">
                                <div class="oe_dropdown_toggle oe_dropdown_kanban" groups="base.group_user">
                                    <span class="oe_e">í</span>
                                    <ul class="oe_dropdown_menu">
                                        <t t-if="widget.view.is_action_enabled('edit')"><li><a type="edit">Tasks Settings</a></li></t>
                                        <t t-if="widget.view.is_action_enabled('delete')"><li><a type="delete">Delete</a></li></t>
                                        <li><ul class="oe_kanban_colorpicker" data-field="color"/></li>
                                    </ul>
                                </div>
                                <div class="oe_kanban_content">
                                    <div><b><field name="name"/></b></div>
                                    <div>
	                                    <field name="duan_id"/><br/>
	                                    <t t-if="record.han_chot.raw_value and record.han_chot.raw_value lt (new Date())" t-set="red">oe_kanban_text_red</t>
	                                    <span t-attf-class="#{red || ''}"><i><field name="han_chot"/></i></span>
	                                </div>
	                                <div class="oe_kanban_bottom_right">
	                                	<t groups="base.group_user">
	                                        <a t-if="record.priority.raw_value > 0" type="object" string="Normal" name="set_high_priority" class="oe_e oe_star_off">7</a>
	                                        <a t-if="record.priority.raw_value == 0" type="object" string="Very Important" name="set_normal_priority" class="oe_e oe_star_on">7</a>
	                                    </t>
                                    	<img t-att-src="kanban_image('res.users', 'image_small', record.user_id.raw_value)" t-att-title="record.user_id.value" width="24" height="24" class="oe_kanban_avatar"/>
                                	</div>
                                </div>
                                <div class="oe_clear"></div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>
        
        <record id="action_congviec" model="ir.actions.act_window">
            <field name="name">Công việc</field>
            <field name="res_model">cong.viec</field>
            <field name="view_type">form</field>
            <field name="domain">[]</field>
            <field name="view_mode">calendar,kanban,tree,form</field>
            <field name="view_id" ref="congviec_calendar_view"/>
            <field name="search_view_id" ref="congviec_search_view"/>
            <field name="context">{'search_default_my_congviec': 1,'active_test': False}</field>
        </record>
        
        <record id="action_project_congviec" model="ir.actions.act_window">
            <field name="name">Công việc</field>
            <field name="res_model">cong.viec</field>
            <field name="view_type">form</field>
            <field name="domain">[]</field>
            <field name="view_mode">calendar,kanban,tree,form</field>
            <field name="view_id" ref="congviec_calendar_view"/>
            <field name="search_view_id" ref="congviec_search_view"/>
            <field name="context">{'search_default_duan_id': [active_id], 'default_duan_id' : active_id,'active_test': False}</field>
        </record>
        
        <record id="action_project_nhomcongviec" model="ir.actions.act_window">
            <field name="name">Công việc</field>
            <field name="res_model">cong.viec</field>

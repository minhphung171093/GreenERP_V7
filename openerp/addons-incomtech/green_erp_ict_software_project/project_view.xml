<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
    	<!-- Trạng thái của dự án -->
        <record id="trangthai_duan_search" model="ir.ui.view">
            <field name="name">trangthai.duan.search</field>
            <field name="model">trangthai.duan</field>
            <field name="arch" type="xml">
                <search string="Trạng thái của dự án">
                   <field name="name" string="Project Stages"/>
                   <filter icon="terp-check" string="Common" name="common" domain="[('case_default', '=', 1)]" help="Stages common to all projects"/>
                </search>
            </field>
        </record>

        <record id="trangthai_duan_edit" model="ir.ui.view">
            <field name="name">trangthai.duan.form</field>
            <field name="model">trangthai.duan</field>
            <field name="arch" type="xml">
                <form string="Trạng thái của dự án" version="7.0">
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="case_default"/>
                        </group>
                        <group>
                            <field name="state"/>
                            <field name="sequence"/>
                            <field name="fold"/>
                        </group>
                    </group>
                    <field name="description" placeholder="Add a description..."/>
                </form>
            </field>
        </record>

        <record id="trangthai_duan_tree" model="ir.ui.view">
            <field name="name">trangthai.duan.tree</field>
            <field name="model">trangthai.duan</field>
            <field name="arch" type="xml">
                <tree string="Trạng thái của dự án">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="action_trangthai_duan" model="ir.actions.act_window">
            <field name="name">Trạng thái của dự án</field>
            <field name="res_model">trangthai.duan</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="trangthai_duan_tree"/>
        </record>
        
        <!-- Trạng thái của nhóm công việc -->
        <record id="trangthai_nhom_congviec_search" model="ir.ui.view">

        </record>

        <record id="duan_tree_view" model="ir.ui.view">
            <field name="name">duan.phanmem.tree</field>
            <field name="model">duan.phanmem</field>
            <field name="field_parent">child_ids</field>
            <field name="arch" type="xml">
                <tree string="Dự án">
                    <field name="message_unread" invisible="1"/>
                    <field name="name"/>
                    <field name="user_id"/>
                    <field name="ngay_bd"/>
                    <field name="ngay_kt"/>
                    <field name="han_chot"/>
                    <field name="muc_thuong"/>
                    <field name="stage_id"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="duan_kanban_view">
            <field name="name">duan.phanmem.kanban</field>
            <field name="model">duan.phanmem</field>
            <field name="arch" type="xml">
                <kanban version="7.0" class="oe_background_grey">
                    <field name="name"/>
                    <field name="user_id"/>
                    <field name="ngay_bd"/>
                    <field name="ngay_kt"/>
                    <field name="han_chot"/>
                    <field name="color"/>
                    <field name="muc_thuong"/>
                    <field name="stage_id"/>
                    <field name="members"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_color_#{kanban_getcolor(record.color.raw_value)} oe_kanban_card oe_kanban_project oe_kanban_global_click">
                                <div class="oe_dropdown_toggle oe_dropdown_kanban" groups="base.group_user">
                                    <span class="oe_e">í</span>
                                    <ul class="oe_dropdown_menu">
                                        <t t-if="widget.view.is_action_enabled('edit')"><li><a type="edit">Project Settings</a></li></t>
                                        <t t-if="widget.view.is_action_enabled('delete')"><li><a type="delete">Delete</a></li></t>
                                        <li><ul class="oe_kanban_colorpicker" data-field="color"/></li>
                                    </ul>
                                </div>
                                <div class="oe_kanban_content">
                                    <h4><field name="name"/></h4>
                                    <div class="oe_kanban_project_list">
                                    	<a name="%(action_project_nhom_congviec)d" type="action" style="margin-right: 10px"> 
                                            <span><field name="nhom_congviec_count"/> Nhóm công việc</span>
                                        </a>
                                        <a name="%(action_project_congviec)d" type="action" style="margin-right: 10px"> 
                                            <span><field name="congviec_count"/> Công việc</span>
                                        </a>
                                    </div>
                                    <div class="oe_kanban_footer_left">
                                        <span t-if="record.han_chot.raw_value">
                                            <span class="oe_e">N</span>
                                            <field name="han_chot"/>
                                        </span>
                                        <span t-if="record.muc_thuong.raw_value">
                                            <field name="muc_thuong"/>
                                        </span>
                                    </div>
                                    <div class="oe_kanban_project_avatars">
                                    	<t t-foreach="record.members.raw_value.slice(0,11)" t-as="member">
                                            <img t-att-src="kanban_image('res.users', 'image_small', member)" t-att-data-member_id="member"/>
                                        </t>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <record id="action_duan" model="ir.actions.act_window">
            <field name="name">Dự án</field>
            <field name="res_model">duan.phanmem</field>
            <field name="view_type">form</field>
            <field name="domain">[]</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_id" ref="duan_kanban_view"/>
            <field name="search_view_id" ref="duan_search_view"/>
            <field name="context">{'search_default_my_project': 1}</field>
        </record>
        
        <record id="duan_form_view_readonly" model="ir.ui.view">
		    <field name="name">duan.phanmem.form.readonly.cust</field>
		    <field name="model">duan.phanmem</field>
		    <field name="inherit_id" ref="duan_form_view"/>
		    <field name="groups_id" eval="[(6, 0, [ref('group_software_project_manager') ])]"/>
		    <field name="arch" type="xml">
		        <field name='muc_thuong' position="attributes">
		            <attribute name="readonly">0</attribute>
		        </field>
		        <field name='han_chot' position="attributes">
		            <attribute name="readonly">0</attribute>
		        </field>
		        <field name='ngay_ht' position="attributes">
		            <attribute name="readonly">0</attribute>
		        </field>
		    </field>
		</record>
		
		<record id="congviec_form_view_readonly" model="ir.ui.view">
		    <field name="name">cong.viec.form.readonly.cust</field>
		    <field name="model">cong.viec</field>
		    <field name="inherit_id" ref="congviec_form_view"/>
		    <field name="groups_id" eval="[(6, 0, [ref('group_software_project_leader') ])]"/>
		    <field name="arch" type="xml">
		        <field name='ngay_ht' position="attributes">
		            <attribute name="readonly">0</attribute>
		        </field>
		    </field>
		</record>
		
		<!-- Tre han -->
		<record id="duan_trehan_tree_view" model="ir.ui.view">
            <field name="name">duan.trehan.tree</field>
            <field name="model">duan.trehan</field>
            <field name="arch" type="xml">
                <tree string="Trễ hạn" editable='bottom'>
                    <field name="name"/>
                    <field name="phantram_giam"/>
                </tree>
            </field>
        </record>
        
        <record id="action_duan_trehan" model="ir.actions.act_window">
            <field name="name">Trễ hạn</field>
            <field name="res_model">duan.trehan</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="duan_trehan_tree_view"/>
        </record>
		
    </data>
</openerp>

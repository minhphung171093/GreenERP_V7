        <record id="bang_gia_mua_tree_view" model="ir.ui.view">
			<field name="name">bang.gia.mua.tree</field>
			<field name="model">bang.gia</field>
			<field name="arch" type="xml">
				<tree string="Bảng giá mua">
					<field name="name"/>
					<field name="currency_id"/>
					<field name="state"/>
					<field name="type" invisible='1'/>
				</tree>
			</field>
		</record>
		
		<record id="bang_gia_filter_view" model="ir.ui.view">
			<field name="name">bang.gia.filter</field>
			<field name="model">bang.gia</field>
            <field name="arch" type="xml">
                <search string="Bảng giá">
                    <field name="name"/>
                    <field name="currency_id"/>
                    <field name="state"/>
                    <group expand="0" string="Group By...">
                    	<filter string="Đơn vị tiền tệ" context="{'group_by': 'currency_id'}"/>
                    	<filter string="Tháng" context="{'group_by': 'name'}"/>
                    	<filter string="Trạng thái" context="{'group_by': 'state'}"/>
                   	</group>
               </search>
            </field>
        </record>
		
		<record id="bang_gia_mua_form_view" model="ir.ui.view">
			<field name="name">bang.gia.mua.form</field>
			<field name="model">bang.gia</field>
            <field name="arch" type="xml">
	            <form string="Bảng giá mua" version="7.0">
	            	<header>
	            		<button name="duyet" string="Duyệt" states="moi_tao" type="object" class="oe_highlight"/>
	            		<button name="huy_bo" string="Hủy bỏ" states="da_duyet" type="object" class="oe_highlight"/>
	            		<button name="chinh_sua_lai" string="Chỉnh sửa lại" states="huy_bo" type="object" class="oe_highlight"/>
						<field name="state" widget="statusbar" statusbar_visible="moi_tao,da_duyet,huy_bo"/>
						<field name="type" invisible='1'/>
	            	</header>
	            	<sheet>
	            		<group col='4' colspan='2'>
		                	<field name="name" on_change="onchange_name(name,currency_id)"/>
							<field name="currency_id" on_change="onchange_name(name,currency_id)"/>
						</group>
						<notebook>
							<page string="Chi tiết bảng giá">
								<field name="banggia_line" nolabel='1'>
									<tree editable='bottom'>
										<field name="product_id"/>
										<field name="gia"/>
									</tree>
								</field>
							</page>
						</notebook>
		         	</sheet>
		         </form>
			</field>
		</record>
          
		<record id="action_bang_gia_mua" model="ir.actions.act_window">
            <field name="name">Bảng giá mua</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">bang.gia</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="bang_gia_mua_tree_view"/>
            <field name="domain">[('type','=','mua')]</field>
            <field name="context">{'default_type':'mua'}</field>
        </record>
        <record model="ir.actions.act_window.view" id="action_bang_gia_mua1">

<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
    	<record id="stock.stock_location_company" model="stock.location">
            <field name="warehouse_id" eval="ref('stock.warehouse0')"/>
        </record>
    	
    	<record id="stock.stock_location_output" model="stock.location">
            <field name="warehouse_id" eval="ref('stock.warehouse0')"/>
        </record>
        
        <record id="stock.stock_location_stock" model="stock.location">
            <field name="warehouse_id" eval="ref('stock.warehouse0')"/>
        </record>
    	
    	<record id="stock.journal_delivery" model="stock.journal">
            <field name="name">Xuất hàng bán</field>
            <field name="source_type">out</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.stock_location_customers'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_delivery')"/>
        </record>
        
        <record id="journal_2" model="stock.journal">
            <field name="name">Nhập hàng mua</field>
            <field name="source_type">in</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_suppliers'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_incoming_shipments')"/>
        </record>
        
        <record id="journal_3" model="stock.journal">
            <field name="name">Chuyển kho nội bộ</field>
            <field name="source_type">internal</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.stock_location_stock')),
            									(4, ref('stock.stock_location_suppliers')),
            									(4, ref('stock.location_inventory')),
            									(4, ref('stock.location_production')),
            									(4, ref('stock.stock_location_scrapped'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_internal')"/>
        </record>
        
        <record id="journal_4" model="stock.journal">
            <field name="name">Xử lý hàng thất thoát</field>
            <field name="source_type">phys_adj</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.location_inventory'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_xuly_thatthoat')"/>
        </record>
        
        <record id="journal_5" model="stock.journal">
            <field name="name">Xử lý hàng hư hỏng</field>
            <field name="source_type">phys_adj</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.stock_location_scrapped'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_xuly_huhong')"/>
        </record>
        
        <record id="journal_6" model="stock.journal">
            <field name="name">Hàng trả của KH</field>
            <field name="source_type">return_customer</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_customers'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_return_customer')"/>
        </record>
        
        <record id="journal_7" model="stock.journal">
            <field name="name">Hàng trả NCC</field>
            <field name="source_type">return_supplier</field>
            <field name="from_location_id" eval="[(4, ref('stock.stock_location_stock'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.stock_location_suppliers'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_return_supplier')"/>
        </record>
        
        <record id="journal_8" model="stock.journal">
            <field name="name">Sản xuất</field>
            <field name="source_type">production</field>
            <field name="from_location_id" eval="[(4, ref('stock.location_production')),(4, ref('stock.stock_location_stock'))]"/>
            <field name="to_location_id" eval="[(4, ref('stock.location_production')),(4, ref('stock.stock_location_stock'))]"/>
            <field name="sequence_id" eval="ref('green_erp_stock.seq_picking_journal_production')"/>
        </record>
        
    </data>
</openerp>
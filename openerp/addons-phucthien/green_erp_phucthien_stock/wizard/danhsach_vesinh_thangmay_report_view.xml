                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="action_danhsach_vesinh_thangmay_report" model="ir.actions.act_window">
            <field name="name">Danh sách vệ sinh thang máy</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">danhmuc.thietbi.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_danhsach_vesinh_thangmay_report"/>
            <field name="target">new</field>
        </record>
        
    </data>
</openerp>


<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="view_danhsach_vesinh_thangmay_report" model="ir.ui.view">
            <field name="name">view.danhsach_vesinh.thangmay.report</field>
            <field name="model">danhmuc.thietbi.report</field>
            <field name="arch" type="xml">
                <form string="Danh sách vệ sinh thang máy" version="7.0">
                    <group >
                    	<group>
                    		<field name="tu_ngay" />
                    		<field name="den_ngay" />
                    	</group>
                    </group>
                    <footer>
                        <button name="print_report_dmtb" string="Print" type="object" class="oe_highlight" />

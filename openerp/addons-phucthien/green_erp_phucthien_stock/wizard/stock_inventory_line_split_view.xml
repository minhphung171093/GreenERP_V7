<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    	<record id="view_split_in_lots_inherit_inherit" model="ir.ui.view">
            <field name="name">stock.inventory.line.split.form.inherit</field>
            <field name="model">stock.inventory.line.split</field>
            <field name="inherit_id" ref="stock.view_split_in_lots_inherit"/>

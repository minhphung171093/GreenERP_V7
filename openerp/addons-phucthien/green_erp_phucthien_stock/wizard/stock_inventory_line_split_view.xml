                <xpath expr="//field[@name='use_exist']" position="attributes">
                	<attribute name="string">Sử dụng lô đã có</attribute>
                </xpath>
            </field>
        </record>
    	
    	<record id="stock.track_line" model="ir.actions.act_window">
            <field name="name">Split in Serial Numbers</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.move.split</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="context">{'default_use_exist':True,'form_view_ref': False}</field>
        </record>
    	
    </data>
</openerp>

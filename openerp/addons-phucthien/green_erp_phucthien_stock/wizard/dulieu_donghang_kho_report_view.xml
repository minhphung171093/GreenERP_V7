<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="view_dulieu_donghang_kho_report" model="ir.ui.view">
            <field name="name">dulieu.donghang.kho.report</field>
            <field name="model">dulieu.donghang.kho.report</field>
            <field name="arch" type="xml">
                <form string="Báo cáo dữ liệu đóng hàng" version="7.0">
                    <group >
                    	<group>
                    		<field name="partner_id" />
                    	</group>
                    	<group>
                    		<field name="tu_ngay" />
                    		<field name="den_ngay" />
                    	</group>
                    </group>
                    <footer>
                        <button name="print_report" string="Print" type="object" class="oe_highlight" context="{'type':'appointments'}"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="action_dulieu_donghang_kho_report" model="ir.actions.act_window">
            <field name="name">Báo cáo dữ liệu đóng hàng</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">dulieu.donghang.kho.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_dulieu_donghang_kho_report"/>
            <field name="target">new</field>
        </record>
        
        <menuitem id="menu_dulieu_donghang" name="Dữ liệu đóng hàng" parent="stock.menu_stock_root" sequence="3"/>
        	<menuitem id="menu_dulieu_donghang_kho_report" name="Báo cáo dữ liệu đóng hàng" action="action_dulieu_donghang_kho_report" parent="green_erp_phucthien_stock.menu_dulieu_donghang" sequence="10"/>
        
    </data>
</openerp>

